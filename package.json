{
  "name": "mongoose-models",
  "version": "0.4.0",
  "description": "An extension to mongoose's models",
  "main": "lib/index.js",
  "scripts": {
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kristianmandrup/mongoose-models"
  },
  "keywords": [
    "mongoose",
    "mongo",
    "db",
    "model"
  ],
  "author": "Kristian Mandrup",
  "license": "MIT",
  "dependencies": {
    "bson": "*",
    "bson-ext": "*",
    "colors": "^1.1.2",
    "fs-readdir-recursive": "^1.0.0",
    "kerberos": "*",
    "mongoose": "^4.9.x",
    "mongoose-types": "~1.0.3",
    "q": "*",
    "underscore.string": "^3.3.4",
    "uuid-v4": "^0.1.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "concurrency": 1,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
