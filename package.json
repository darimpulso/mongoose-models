{
    "name": "mongoose-models",
    "version": "0.3.1",
    "description": "A extension to mongoose's models",
    "main": "lib/index.js",
    "scripts": {
        "test": "ava"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/SportZing/mongoose-models"
    },
    "keywords": [
        "mongoose",
        "mongo",
        "db",
        "model"
    ],
    "author": "James Brumond",
    "license": "MIT",
    "dependencies": {
        "mongoose": "^4.9.x",
        "mongoose-types": "~1.0.3",
        "fs-readdir-recursive": "^1.0.0",
        "uuid-v4": "^0.1.0",
        "colors": "^1.1.2",
        "bson-ext": "*",
        "bson": "*",
        "q": "*",
        "kerberos": "*"
    },
    "devDependencies": {
        "ava": "^0.19.1",
        "babel-cli": "^6.24.0",
        "babel-core": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-env": "^1.3.1"
    },
    "babel": {
        "presets": [
            "env"
        ],
        "plugins": [
            "transform-runtime"
        ]
    },
    "ava": {
        "files": [
            "test/*.test.js"
        ],
        "concurrency": 1,
        "failFast": true,
        "failWithoutAssertions": false,
        "tap": true,
        "powerAssert": false,
        "require": [
            "babel-register",
            "babel-polyfill"
        ],
        "babel": "inherit"
    }
}